func _on_TextureButton_pressed():
	get_tree().change_scene(\"res://Swimming.tscn\")
func _process(delta):
	var Dinghy = get_tree().get_nodes_in_group(\"DiveButton\")[0]
	var Bubbles = get_tree().get_nodes_in_group(\"DiveButton\")[1]
	if abs(Dinghy.position.x - Bubbles.position.x) < 100 and abs(Dinghy.position.y - Bubbles.position.y) < 100 :
		get_node(\"TextureButton\").disabled = false
		get_node(\"TextureButton\").visible = true
	else:
		get_node(\"TextureButton\").disabled = true
		get_node(\"TextureButton\").visible = false"

